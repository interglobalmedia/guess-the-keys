{
  "name": "guess-the-keys",
  "version": "1.0.0",
  "description": "## A fun little game made with JavaScript, SCSS/CSS3, and HTML.",
  "main": "keypress.js",
  "scripts": {
    "scss": "node-sass --watch styles/scss -o styles/",
    "clean": "rm -rf dist/ && mkdir -p dist/styles/",
    "cleanSCSS": "rm -rf dist/styles/scss/",
    "build": "cp -R scripts dist/ && cp -R styles/ dist/styles/ && cp favicon.ico index.html dist/",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interglobalmedia/guess-the-keys.git"
  },
  "author": "Maria D. Campbell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/interglobalmedia/guess-the-keys/issues"
  },
  "homepage": "https://github.com/interglobalmedia/guess-the-keys#readme",
  "dependencies": {
    "gh-pages": "^3.2.3",
    "node-sass": "^7.0.1"
  }
}
